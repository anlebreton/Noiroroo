import java.util.Vector;


public class Classe {

	private String name;
	private String description;
	private Vector<Double> statPerception;
	private Vector<Double> statBrute;
	private Vector<Vector <String>> competences; 
	
	public Classe(String name, Vector<Double> statPerception, Vector<Double> statBrute, Vector<Vector <String>> competences)
	{
		this.name = name;
		this.statPerception = statPerception;
		this.statBrute = statBrute;
		this.competences = competences;
		
		
	}
	
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Double getPrecision() {
		return statPerception.get(0);
	}

	public void setPrecision(Double precision) {
		this.statPerception.set(0, precision);
	}

	public Double getChance() {
		return statPerception.get(1);
	}

	public void setChance(Double chance) {
		this.statPerception.set(1, chance);
	}

	public Double getEsquive() {
		return statPerception.get(2);
	}

	public void setEsquive(String esquive) {
		this.esquive = esquive;
	}

	public String getHabilité() {
		return habilite;
	}

	public void setHabilite(String habilite) {
		this.habilite = habilite;
	}

	public String getNtelligence() {
		return intelligence;
	}

	public void setIntelligence(String intelligence) {
		this.intelligence = intelligence;
	}

	public String getFurtivité() {
		return furtivite;
	}

	public void setFurtivite(String furtivite) {
		this.furtivite = furtivite;
	}

	public String getCharisme() {
		return charisme;
	}

	public void setCharisme(String charisme) {
		this.charisme = charisme;
	}

	public String getVie() {
		return vie;
	}

	public void setVie(String vie) {
		this.vie = vie;
	}

	public String getForce() {
		return force;
	}

	public void setForce(String force) {
		this.force = force;
	}

	public String getRapidite() {
		return rapidite;
	}

	public void setRapidite(String rapidite) {
		this.rapidite = rapidite;
	}

	public String getDexterite() {
		return dexterite;
	}

	public void setDexterite(String dexterite) {
		this.dexterite = dexterite;
	}

	public String getResistance() {
		return resistance;
	}

	public void setResistance(String resistance) {
		this.resistance = resistance;
	}

	public String getEsprit() {
		return esprit;
	}

	public void setEsprit(String esprit) {
		this.esprit = esprit;
	}

	public Vector<Double> getStatPerception() {
		return statPerception;
	}

	public void setStatPerception(Vector<Double> statPerception) {
		this.statPerception = statPerception;
	}

	public Vector<Double> getStatBrute() {
		return statBrute;
	}

	public void setStatBrute(Vector<Double> statBrute) {
		this.statBrute = statBrute;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public Vector<Vector <String>> getCompetences() {
		return competences;
	}

	public void setCompetences(Vector<Vector <String>> competences) {
		this.competences = competences;
	}
	
	
	
}
