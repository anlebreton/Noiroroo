
public class De {
	private int type;
	private int number;
	public De(String de) {
		// TODO Auto-generated constructor stub
		String[]stringArray = new String[2];

		stringArray = de.split("D");
		type = Integer.parseInt(stringArray[1]);
		number = Integer.parseInt(stringArray[0]);
		
		System.out.println(stringArray[0]);
		
		
		
	}
}
