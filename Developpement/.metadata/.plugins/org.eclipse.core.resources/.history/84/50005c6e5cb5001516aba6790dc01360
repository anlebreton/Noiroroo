
public class Main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] tabl = new int[6];
		for (int i = 0; i < 100; i++){
			De newde = new De("2D6");
			tabl[newde.getResult()-1]++;
		}
		
		for (int i = 0; i < tabl.length; i++) {
			System.out.println(i + " : " + tabl[i]);
		}

	}

}
